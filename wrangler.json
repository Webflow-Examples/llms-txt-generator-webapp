{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "webflow-llms",
  "main": "./dist/_worker.js/index.js",
  "compatibility_date": "2024-04-15",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist"
  },
  "observability": {
    "enabled": true
  },
  "kv_namespaces": [
    {
      "binding": "WEBFLOW_CONTENT",
      "id": "1234"
    },
    {
      "binding": "EXPOSURE_SETTINGS",
      "id": "1234"
    }
  ]
}
